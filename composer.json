{
    "name": "nandanprac/buzz-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle for using the lightweight Buzz HTTP client.",
    "keywords": ["buzz", "http client"],
    "license": "MIT",
    "authors": [
        {
            "name": "Gremo",
            "homepage": "https://github.com/gremo"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/dependency-injection": "~2.0|~3.0|~4.0",
        "symfony/http-kernel": "~2.0|~3.0|~4.0",
        "symfony/config": "~2.0|~3.0",
        "practo/buzz": "dev-master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nandanprac/GremoBuzzBundle.git"
        }
     ],
    "autoload": {
        "psr-4": { "Gremo\\BuzzBundle\\": "" }
    }
}
